.nav {
    background: #232323;
    height: 60px;
    display: inline-block;
}

.nav li {
    float: left;
    list-style-type: none;
    position: relative;
}

.nav li a {
    font-size: 16px; 
    color: white;
    display: block;
    line-height: 60px;
    padding: 0 26px;
    text-decoration: none;
    border-left: 1px solid #2e2e2e;
    font-family: Montserrat, sans-serif;
    text-shadow: 0 0 1px rgba(255, 255, 255, 0.5);
}
.nav li a:hover {
    background-color: #2e2e2e;
}
#search {
    width: 357px;
    margin: 4px;
}
#search_text{
    width: 297px;
  /*  padding: 15px 0 15px 20px;*/
    font-size: 16px;
    font-family: Montserrat, sans-serif;
    border: 0 none;
    height: 52px;
    margin-right: 0;
    color: white;
    outline: none;
    background: #ba5d3b;
    float: left;
    box-sizing: border-box;
    transition: all 0.15s;
}
::-webkit-input-placeholder { /* WebKit browsers */
    color: white;
}
:-moz-placeholder { /* Mozilla Firefox 4 to 18 */
    color: white;
}
::-moz-placeholder { /* Mozilla Firefox 19+ */
    color: white;
}
:-ms-input-placeholder { /* Internet Explorer 10+ */
    color: white;
}
#search_text:focus {
    background: #bd883d;
}
#search_button {
    border: 0 none;
    background: #ba5d3b url(search.png) center no-repeat;
    width: 60px; 
    float: left;
    padding: 0;
    text-align: center;
    height: 52px;
    cursor: pointer;
}
/*columns with icons*/
.items-block-left, .items-block-right{
    float:left;
    padding:10px 0;
    width: 300px;
}
.items-block-right{
    padding-right:0;
}
.items-block-left{
    margin-right:20px;
}
.icon-list li{
    float:left;
    width:80px;
    margin-bottom: 10px;
}
.icon-list li:last-child{
    padding-right:0;
}
.icon-list li a{
    opacity: 0.4;
    border-radius: 5px 5px 5px 5px;
    display: block;
    padding: 4px;
    background:#999;
}
.icon-list li a.active{
    background-color: #EFE08B;
    border: 2px solid #BDB206;
    opacity: 0.7;
}
.icon-list li a.selected{
    border: 2px solid #2142A0;
    opacity: 0.7;
}
